<!ENTITY networkProviderParameters '
<param xmlns="http://wadl.dev.java.net/2009/02" required="false"
    name="provider:physical_network" style="plain" type="xsd:string">
    <wadl:doc xmlns:wadl="http://wadl.dev.java.net/2009/02"
